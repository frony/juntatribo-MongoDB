{
  "name": "jukebox",
  "version": "0.1.0",
  "private": true,
  "description": "An app to share music lists, gather news about bands, list upcoming concerts and save comments and images of concerts I attend.",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "pretest": "npm run lint",
    "test": "mocha",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "clean:install": "rm -Rf node_modules && npm install",
    "diagnose": "npm run clean:install && npm test && npm outdated && npm run coverage:open"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:frony/jukebox.git"
  },
  "keywords": [
    "Node",
    "music",
    "jukebox",
    "concert",
    "concerts",
    "band",
    "bands"
  ],
  "author": "Marcos Frony <mfrony@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/frony/jukebox/issues"
  },
  "homepage": "https://github.com/frony/jukebox#readme",
  "dependencies": {
    "axios": "0.18.0",
    "bcrypt": "3.0.6",
    "bluebird": "3.5.4",
    "body-parser": "1.19.0",
    "bootstrap": "3.3.7",
    "chalk": "2.4.2",
    "cookie-parser": "1.4.4",
    "debug": "4.1.1",
    "ejs": "2.6.1",
    "express": "4.17.0",
    "express-ejs-layouts": "2.5.0",
    "express-session": "1.16.1",
    "express-validator": "5.3.1",
    "jquery": "3.4.1",
    "mongodb": "3.2.5",
    "morgan": "1.9.1",
    "passport": "0.4.0",
    "passport-local": "1.0.0",
    "path": "0.12.7",
    "xml2js": "0.4.19"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.17.2",
    "mocha": "6.1.4",
    "npm": "6.9.0"
  }
}
